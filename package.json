{
  "name": "dev-framework",
  "version": "1.0.0",
  "description": "A small framework for my sites. Might be opinionated.",
  "readme": "README.md",
  "main": "index.js",
  "scripts": {
    "test": "",
    "clean": "rimraf build",
    "dev": "NODE_ENV=development webpack-dashboard -c cyan -- node server",
    "build:webpack": "NODE_ENV=production webpack --config webpack/production.js --color -p",
    "build": "npm run clean && npm run build:webpack"
  },
  "author": "Jeff Leplomet <jleplomet@gmail.com>",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "bundle-loader": "^0.5.4",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "ip": "^1.1.3",
    "ngrok": "^2.2.2",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.10.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "sw-precache": "^4.0.0",
    "sw-toolbox": "^3.2.1",
    "webpack": "^1.13.2",
    "webpack-dashboard": "0.1.8",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^3.2.1",
    "fbemitter": "^2.0.2",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-transition-group": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-media": "^1.2.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
  }
